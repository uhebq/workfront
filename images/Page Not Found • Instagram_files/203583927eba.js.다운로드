//  Image Fallback

function openDropdown(e) {
    $(e.target)
        .parents('.has-dropdown')
        .toggleClass("dropdown-open")
        .children('a').toggleClass("link-active");
}

$(document).ready(function() {
    $(".has-dropdown > a").live('click', openDropdown);
    var $html = $('html');
    if ($.browser.msie) {
        $html.addClass('msie');
    } else if ($.browser.opera) {
        $html.addClass('opera');
    }
});
